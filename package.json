{
  "name": "doc2ts",
  "version": "0.1.1",
  "description": "根据接口文档生成ts接口文件",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "doc2ts": "./bin/doc2api.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "node ./bin/doc2api.js",
    "init-doc": "node ./bin/init.js",
    "start": "nodemon --watch ./src/test.ts --ext ts --exec nodemon src/test.ts"
  },
  "author": "space-77",
  "license": "MIT",
  "keywords": [
    "doc2ts",
    "swagger",
    "API"
  ],
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "js-crypto-rsa": "^1.0.4",
    "pont-engine": "^1.3.1",
    "ts-node": "^10.7.0",
    "commander": "^9.2.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "nodemon": "^2.0.15"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "src/api/*",
      "examples/*"
    ]
  }
}
