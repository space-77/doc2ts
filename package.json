{
  "name": "doc2ts",
  "version": "0.0.2",
  "description": "根据接口文档生成ts接口文件",
  "main": "src/index.ts",
  "bin": {
    "doc2ts-build": "npm run build"
  },
  "scripts": {
    "build": "ts-node src/index.ts",
    "start": "nodemon --watch ./src/index.ts --ext ts --exec nodemon src/index.ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.26.1",
    "js-crypto-rsa": "^1.0.4",
    "pont-engine": "^1.3.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "nodemon": "^2.0.15"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "src/api/*",
      "examples/*"
    ]
  }
}
