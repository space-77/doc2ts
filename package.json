{
  "name": "doc2ts",
  "version": "1.1.19-beta.3",
  "description": "根据接口文档生成ts接口文件",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "dots": "bin/dots.js",
    "doc2ts": "bin/doc2api.js"
  },
  "scripts": {
    "build": "ts-node ./scripts/build.ts && tsc",
    "test": "node ./bin/doc2api.js",
    "init-doc": "ts-node ./src/dev-init.ts",
    "dev-manage": "ts-node src/scripts/manage-dev.ts",
    "init-config": "ts-node ./src/scripts/initConfig.ts",
    "dev": "ts-node ./src/dev.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/space-77/doc2ts"
  },
  "bugs": {
    "url": "https://github.com/space-77/doc2ts/issues"
  },
  "homepage": "https://github.com/space-77/doc2ts",
  "author": "space-77",
  "license": "MIT",
  "keywords": [
    "API",
    "util",
    "types",
    "convert",
    "openapi",
    "swagger",
    "typescript",
    "openapi to js"
  ],
  "dependencies": {
    "axios": "*",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "commander": "^8.3.0",
    "compare-versions": "^6.1.0",
    "doc-pre-data": "^1.1.19-beta.3",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.4",
    "is-ecma-keyword": "^1.0.3",
    "jsonrepair": "^3.8.0",
    "lodash": "*",
    "ora": "^5.4.1",
    "prettier": "^3.3.2",
    "qs": "*",
    "simple-git": "^3.25.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/lodash": "^4.14.189",
    "@types/md5": "^2.3.5",
    "@types/node": "^17.0.25",
    "@types/prettier": "^2.6.0",
    "@types/qs": "*",
    "fs-extra": "^10.1.0",
    "md5": "^2.3.0",
    "nodemon": "^2.0.15"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "src/api/*",
      "examples/*"
    ]
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "endOfLine": "lf",
    "printWidth": 120,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none",
    "stylelintIntegration": true
  }
}
